{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "import/extensions": 0,
    "react/prop-types": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 0,
    "no-unused-expressions": 0,
    "no-return-assign": 0
  }
}
